<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.onlinetest">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:usesCleartextTraffic="true">
        <activity android:name=".logInPage.HomePage"/>
        <activity android:name=".logInPage.LogInPage" />
        <activity android:name=".result.CheckJawaban" />
        <activity android:name=".result.CheckMyResult" />
        <activity android:name=".logInPage.RegisterPage" />
        <activity android:name=".MainActivity" />
        <activity android:name=".logInPage.Beranda" />
        <activity android:name=".LembarSoal">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>